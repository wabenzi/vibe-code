{
  "name": "aws-test",
  "version": "1.0.0",
  "description": "AWS serverless user management API using Effect TypeScript, API Gateway, Lambda, and DSQL",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "synth": "cdk synth",
    "destroy": "cdk destroy",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap",
    "local:start": "./scripts/local-dev.sh start",
    "local:stop": "./scripts/local-dev.sh stop",
    "local:deploy": "npm run local:cdk -- deploy",
    "local:deploy:simple": "npm run local:cdk:simple -- deploy",
    "local:destroy": "npm run local:cdk -- destroy",
    "local:bootstrap": "npm run local:cdk -- bootstrap",
    "local:cdk": "source .env.local && cdk --app 'npx ts-node --prefer-ts-exts bin/local-app.ts'",
    "local:cdk:simple": "source .env.local && cdk --app 'npx ts-node --prefer-ts-exts bin/simple-local-app.ts'",
    "local:test": "./scripts/local-dev.sh test",
    "local:logs": "./scripts/local-dev.sh logs",
    "local:reset": "./scripts/local-dev.sh reset",
    "test:integration": "jest --config jest.integration.config.js"
  },
  "keywords": [
    "aws",
    "lambda",
    "api-gateway",
    "dsql",
    "effect-ts",
    "typescript",
    "serverless"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@aws-cdk/aws-lambda-nodejs": "^1.203.0",
    "@aws-sdk/rds-signer": "^3.848.0",
    "@effect/platform": "^0.90.0",
    "@effect/schema": "^0.75.5",
    "@types/pg": "^8.15.4",
    "aws-cdk-lib": "^2.207.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.4.2",
    "effect": "^3.17.1",
    "pg": "^8.16.3",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "aws-cdk": "^2.1022.0",
    "esbuild": "^0.25.8",
    "jest": "^30.0.5",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
