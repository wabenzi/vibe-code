{
  "name": "aws-test",
  "version": "1.0.0",
  "description": "AWS serverless user management API using Effect TypeScript, API Gateway, Lambda, and DSQL",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:unit": "jest --testPathPatterns=unit",
    "test:integration": "jest --config jest.integration.config.js",
    "test:contract": "jest --testPathPatterns=contract",
    "test:behavioral": "cucumber-js",
    "test:api": "jest --testPathPatterns=api",
    "test:all:jest": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --config jest.ci.config.js --coverage --ci --watchAll=false --passWithNoTests",
    "cdk": "source .env 2>/dev/null || true && cdk",
    "deploy": "source .env 2>/dev/null || true && cdk deploy",
    "deploy:export": "./scripts/deploy-and-export.sh",
    "deploy:local": "source .env.local && ./scripts/deploy-and-export.sh",
    "test:with-deployment": "./scripts/deploy-and-export.sh && source .env.deployment && npm run test:integration",
    "synth": "source .env 2>/dev/null || true && cdk synth",
    "destroy": "source .env 2>/dev/null || true && cdk destroy",
    "diff": "source .env 2>/dev/null || true && cdk diff",
    "bootstrap": "source .env 2>/dev/null || true && cdk bootstrap",
    "local:start": "./scripts/development/local-dev.sh start",
    "local:stop": "./scripts/development/local-dev.sh stop",
    "local:deploy": "npm run local:cdk -- deploy",
    "local:destroy": "npm run local:cdk -- destroy",
    "local:bootstrap": "npm run local:cdk -- bootstrap",
    "local:cdk": "source .env.local && cdklocal",
    "local:test": "./scripts/development/local-dev.sh test",
    "local:logs": "./scripts/development/local-dev.sh logs",
    "local:reset": "./scripts/development/local-dev.sh reset",
    "local:status": "./scripts/development/local-dev.sh status",
    "deploy:localstack": "scripts/deployment/deploy-localstack.sh",
    "deploy:localstack:teardown": "scripts/deployment/deploy-localstack.sh teardown",
    "deploy:localstack:status": "scripts/deployment/deploy-localstack.sh status",
    "deploy:aws": "scripts/deployment/deploy-aws.sh",
    "deploy:aws:teardown": "scripts/deployment/deploy-aws.sh teardown",
    "deploy:aws:status": "scripts/deployment/deploy-aws.sh status",
    "test:deployment": "scripts/testing/test-deployment.sh",
    "test:deployment:cleanup": "scripts/testing/test-deployment.sh cleanup",
    "test:deployment:check": "scripts/testing/test-deployment.sh check",
    "test:localstack": "scripts/testing/test-localstack.sh",
    "test:localstack:cleanup": "scripts/testing/test-localstack.sh cleanup",
    "test:all": "scripts/testing/test-all.sh",
    "test:all:localstack-only": "scripts/testing/test-enterprise.sh",
    "test:all:aws": "scripts/testing/test-all.sh aws",
    "test:all:localstack": "scripts/testing/test-all.sh localstack",
    "test:all:performance": "scripts/testing/test-all.sh performance",
    "test:enterprise": "scripts/testing/test-enterprise.sh",
    "test:enterprise:unit": "scripts/testing/test-enterprise.sh unit",
    "test:enterprise:integration": "scripts/testing/test-enterprise.sh integration",
    "test:enterprise:contract": "scripts/testing/test-enterprise.sh contract",
    "test:enterprise:behavioral": "scripts/testing/test-enterprise.sh behavioral",
    "test:enterprise:ci": "scripts/testing/test-enterprise.sh ci",
    "docs:validate": "scripts/development/docs.sh validate",
    "docs:serve": "scripts/development/docs.sh serve",
    "docs:generate": "scripts/development/docs.sh generate",
    "docs:generate:typescript": "scripts/development/docs.sh generate typescript-axios",
    "validate:openapi": "scripts/development/docs.sh validate",
    "lint": "echo 'Linting TypeScript code...' && tsc --noEmit && echo 'TypeScript compilation check passed âœ…'",
    "lint:check": "tsc --noEmit --pretty",
    "lint:fix": "echo 'No linter configured for auto-fix. Consider adding ESLint or Prettier.'",
    "coverage": "jest --coverage --coverageReporters=text --coverageReporters=html",
    "coverage:report": "jest --coverage --coverageReporters=html && open coverage/index.html",
    "sloc": "scripts/generate-sloc-report.sh",
    "sloc:report": "scripts/generate-sloc-report.sh && cat SLOC_REPORT.md",
    "clean": "rm -rf dist/ build/ node_modules/ coverage/ cdk.out/ clients/ tmp/ temp/ *.tmp *.log .cache/ cache/ **/*.tsbuildinfo && npm install"
  },
  "keywords": [
    "aws",
    "lambda",
    "api-gateway",
    "dsql",
    "effect-ts",
    "typescript",
    "serverless"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.855.0",
    "@aws-sdk/client-lambda": "^3.855.0",
    "@aws-sdk/credential-providers": "^3.855.0",
    "@aws-sdk/lib-dynamodb": "^3.855.0",
    "@effect/platform": "^0.90.0",
    "@trunkio/launcher": "^1.3.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.4",
    "aws-cdk-lib": "^2.207.0",
    "aws-lambda": "^1.0.7",
    "axios": "^1.7.9",
    "constructs": "^10.4.2",
    "effect": "^3.17.2",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.8.0",
    "@cucumber/pretty-formatter": "^1.0.1",
    "@openapitools/openapi-generator-cli": "^2.15.0",
    "@pact-foundation/pact": "^13.1.3",
    "@types/aws-lambda": "^8.10.152",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.2",
    "aws-cdk": "^2.1022.0",
    "aws-sdk-client-mock": "^4.1.0",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.8",
    "jest": "^30.0.5",
    "nock": "^13.5.5",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "swagger-ui-dist": "^5.17.14",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
